@model List<Order>

<h1>My Orders</h1>

<table>
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Net Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.Id</td>
                <td>@order.Items.Sum(i => i.NetCost)</td>
            </tr>
        }
    </tbody>
</table>
